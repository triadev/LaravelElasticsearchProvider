{
    "name": "triadev/laravel-elasticsearch-provider",
    "description": "A service provider for laravel to managing data versions in elasticsearch.",
    "keywords": ["Elasticsearch", "Laravel", "Lumen"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Lorke",
            "email": "christopher.lorke@gmx.de"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.4",
        "elasticsearch/elasticsearch": "^5.2",
        "triadev/laravel-prometheus-exporter": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.0",
        "laravel/laravel": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Triadev\\Es\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Triadev\\Es\\Provider\\ElasticsearchServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    }
}
